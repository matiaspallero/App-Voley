{
  "expo": {
    "name": "App-Voley",
    "slug": "App-Voley",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/voley-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "assets/voley-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/voley-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "assets/voley-icon.png"
    },
    "plugins": [
      "expo-font",
      "expo-audio"
    ]
  }
}
